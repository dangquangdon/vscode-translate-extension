{
  "name": "cyg-translate",
  "publisher": "dangquangdon",
  "displayName": "cyg-translate",
  "description": "Helper extension to translate English to Finnish text during development.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.48.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cyg-translate.finnishIt"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cyg-translate.finnishIt",
        "title": "Finnish It!"
      }
    ],
    "configuration": {
      "title": "CYG-Translate",
      "properties": {
        "cyg.googleCloudCredentialPath": {
          "type": "string",
          "description": "Absolute path to your google JSON service account credential"
        }
      }
    },
    "keybindings": [
      {
        "command": "cyg-translate.finnishIt",
        "key": "ctrl+f1",
        "mac": "cmd+f1",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "cyg-translate.finnishIt",
          "when": "editorHasSelection"
        }
      ],
      "editor/title": [
        {
          "command": "cyg-translate.finnishIt",
          "when": "editorHasSelection",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.6.0",
    "@types/vscode": "^1.48.0",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "@google-cloud/translate": "^6.0.2",
    "@types/mock-fs": "^4.10.0",
    "mock-fs": "^4.13.0"
  }
}
